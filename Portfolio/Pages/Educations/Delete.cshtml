@page "{educationId:int?}"
@model Portfolio.DeleteEducationModel
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete Education</h1>

<div class="alert alert-danger">
    Are you sure you want to delete @Model.Education.Title title from @Model.Education.University ?
</div>

<form method="post">
    <button type="submit" class="btn btn-danger">Yes!</button>
    <a asp-page="/index" class="btn btn-info">Cancel</a>
</form>

